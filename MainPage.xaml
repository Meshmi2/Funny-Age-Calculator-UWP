        <Button Content="reset" Margin="17,455,0,0" VerticalAlignment="Top" Click="Button_Click_3"/>
        <Button Content="exit" Margin="444,455,0,0" VerticalAlignment="Top" Click="Button_Click_4"/>
        <Button Content="print" Margin="224,456,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>
        <TextBox x:Name="ResultTextBox" HorizontalAlignment="Left" Margin="23,231,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Height="217" Width="457" TextChanged="TextBox_TextChanged"/>
    </Grid>
</Page>